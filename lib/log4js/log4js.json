{
  "customBaseDir" :"/tmp/log4js/",
  "customDefaultAtt" :{
    "type": "dateFile",
    "absolute": true,
    "alwaysIncludePattern": true
  },
  "appenders": [
    {
      "pattern": "/yyyy-MM-dd.txt",
      "category": "logInfo",
      "filename": "info",
      "type": "console",
      "layout": {
        "type": "pattern",
        "pattern": "%r [%x{name}] %p - %m%n",
        "tokens": {
          "name": "MidProxy"
        }
      }
    },
    {
      "pattern": "/yyyy-MM-dd.txt",
      "category": "logErr",
      "filename": "err",
      "type": "console",
      "layout": {
        "type": "pattern",
        "pattern": "%[%r [%x{name}] %p -%] %m%n",
        "tokens": {
          "name": "MidProxy"
        }
      }
    },
    {
      "pattern": "/yyyy-MM-dd.txt",
      "category": "trace",
      "filename": "trace",
      "type": "console",
      "layout": {
        "type": "pattern",
        "pattern": "%[%r [%x{name}] %p -%] %m%n",
        "tokens": {
          "name": "MidProxy"
        }
      }
    }
  ],
  "replaceConsole": true,
  "levels":{ "trace": "TRACE", "logInfo": "INFO", "logErr": "ERROR"}
}